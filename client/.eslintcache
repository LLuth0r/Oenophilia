e\n               wines={wines}\n               handleUpdate={handleUpdate}\n               />\n           </Route>\n           <Route path='/wine/:id'>\n               <WineDetail\n                wines={wines}\n                />\n           </Route>\n           <Route exact path='/wines/add'>\n               <CreateWine\n               handleCreate={handleCreate}\n               currentUser={currentUser}\n               />\n           </Route>\n           <Route exact path='/profile'>\n               <ProfilePage\n               wines={wines}\n               handleDelete={handleDelete}\n               currentUser={currentUser}\n               />\n           </Route>\n       </Switch>\n    )\n}\n","/home/pak/Documents/General_Assembly/Unit_4/P4/The-Cellar/client/src/screens/CreateWine/CreateWine.jsx",[],"/home/pak/Documents/General_Assembly/Unit_4/P4/The-Cellar/client/src/services/wines.js",[],"/home/pak/Documents/General_Assembly/Unit_4/P4/The-Cellar/client/src/screens/Wines/Wines.jsx",["93"],"import React from 'react'\nimport {NavLink} from 'react-router-dom';\n\nexport default function Wines(props) {\n    const { wines, currentUser } = props;\n\n    return (\n        <div className='extreme_fuckery'>\n            {wines.map(wine=> (\n                <React.Fragment key={wine.id}>\n                    <NavLink to={`/wines/${wine.id}`}>\n                        <p>{wine.name}</p>\n                        </NavLink>\n                </React.Fragment>\n            ))}\n        </div>\n    )\n}\n","/home/pak/Documents/General_Assembly/Unit_4/P4/The-Cellar/client/src/screens/WineDetail/WineDetail.jsx",[],"/home/pak/Documents/General_Assembly/Unit_4/P4/The-Cellar/client/src/screens/EditWine/EditWine.jsx",[],"/home/pak/Documents/General_Assembly/Unit_4/P4/The-Cellar/client/src/screens/ProfilePage/ProfilePage.jsx",[],"/home/pak/Documents/General_Assembly/Unit_4/P4/The-Cellar/client/src/components/Carousel/Carousel.jsx",[],"/home/pak/Documents/General_Assembly/Unit_4/P4/